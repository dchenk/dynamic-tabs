{
	"presets": [
		["env", { "modules": false }],
		"stage-1"
	],
	"plugins": [
		[
			"transform-runtime",
			{
				"polyfill": false,
				"regenerator": true
			}
		]
	],
	"compact": "true",
	"env": {
		"production": {},
		"test": {
			"plugins": [
				"dynamic-import-node",
				"transform-es2015-modules-commonjs"
			],
			"sourceMaps": "both"
		}
	}
}
