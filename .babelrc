{
  "presets": [
    ["env", { "modules": false }],
    "stage-1"
  ],
  "plugins": [
    [
      "transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }
    ]
  ],
  "compact": "true",
  "env": {
    "production": {
      "plugins": [
        "lodash",
        "array-includes",
        "transform-flow-strip-types",
        "transform-object-assign",
        "transform-react-remove-prop-types",
        [
          "transform-imports",
          {
            "lodash": {
              "transform": "lodash/${member}",
              "preventFullImport": true
            }
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "dynamic-import-node",
        "transform-es2015-modules-commonjs"
      ],
      "sourceMaps": "both"
    }
  }
}
